                                        <h4 class="header-title">Dropzone File Upload</h4>
                                        <p class="sub-header">
                                            DropzoneJS is an open source library that provides drag’n’drop file uploads with image previews.
                                        </p>
            <!-- this is section hold the area that is responsible for file uploads -->

                                        <form action="/addProperty" method="POST" enctype="multipart/form-data" class="dropzone" id="myAwesomeDropzone" data-plugin="dropzone" data-previews-container="#file-previews"
                                            data-upload-preview-template="#uploadPreviewTemplate">

                                            <div class="form-group mb-3">
                                                <label class="form-label" for="Pname">PROPERTY NAME</label>
                                                <input class="form-control" type="text" id="pname" name="pname" required="" placeholder="Enter the property Name">
                                            </div>
                                            <div class="form-group mb-3">
                                                <label class="form-label" for="address"> ADDRESS </label>
                                                <input class="form-control" type="text" id="address" name="address" required="" placeholder="Enter the house address here">
                                            </div>
                                        
                                            <div class="form-group mb-3">
                                                <label class="form-label" for="property type"> PROPERTY TYPE</label>
                                                
                                                <select class="form-control" id="propertytype" name="propertytype">
                                                    <option value="house">House</option>
                                                    <option value="land">Land</option>
                                                    <option value="shortlets">Shortlets</option>
                                                  </select>
                                            </div>
                                        
                                            <div class="form-group mb-3">
                                                <label class="form-label" for="amount"> AMOUNT</label>
                                                <input class="form-control" type="number" id="amount" name="amount" required="" placeholder="">
                                            
                                            </div>
                                         

                                            <div class="form-group mb-3">
                                                <label class="form-label" for="email"> PRROPERTY DISCRIPTION</label>
                                                <textarea id="description" name="description" rows="4" cols="50" placeholder="Type your message here..."></textarea><br><br>
                                            
                                            </div>


                                            <div class="fallback">
                                                <input  type="file" multiple id="images" name="images" accept="image/*" multiple required/>
                                            </div>

                                            <div class="dz-message needsclick">
                                                <i class="h1 text-muted ri-upload-cloud-2-line"></i>
                                                <h3>Drop files here or click to upload.</h3>
                                                <span class="text-muted font-size-13">(This is just a demo dropzone. Selected files are
                                                    <strong>not</strong> actually uploaded.)</span>
                                            </div>
                                            <div style="margin-left: 45%; width: 200px;" >
                                                <button type="submit" class="btn btn-primary waves-effect waves-light">upload</button>
                                            </div>

                                        </form>

                                        <!-- Preview -->
                                        <div class="dropzone-previews mt-3" id="file-previews"></div>  

                                    </div> <!-- end card-body-->
